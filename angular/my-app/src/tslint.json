{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "no-redundant-jsdoc": true,
    "no-output-on-prefix": true,
    "directive-class-suffix": true
  }
}
